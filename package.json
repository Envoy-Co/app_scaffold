{
  "name": "app_scaffold",
  "version": "1.0.0",
  "description": "A scaffold for developers to build ZAF v2 apps",
  "keywords": [
    "zendesk",
    "app",
    "framework"
  ],
  "author": "Zendesk",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "start": "zendesk-app-scripts start",
    "build": "zendesk-app-scripts build",
    "test": "zendesk-app-scripts test"
  },
  "devDependencies": {
    "@zendeskgarden/css-bedrock": "^7.0.3",
    "@zendesk/zendesk-app-scripts": "^0.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ],
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "postcss-import": {},
      "precss": {}
    }
  }
}
